<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-catalog="bookshop" match-name="author"/>
  <table-filter match-catalog="bookshop" match-name="book"/>
  <table-filter match-catalog="bookshop" match-name="book_to_author"/>
  <table-filter match-catalog="bookshop" match-name="customer"/>
  <table-filter match-catalog="bookshop" match-name="genre"/>
  <table-filter match-catalog="bookshop" match-name="order_items"/>
  <table-filter match-catalog="bookshop" match-name="orders"/>
  <table-filter match-catalog="bookshop" match-name="publ_house"/>
  <table-filter match-catalog="bookshop" match-name="shipping_method"/>
</hibernate-reverse-engineering>